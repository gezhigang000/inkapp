<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.weixin.aidaily</string>

    <key>ProgramArguments</key>
    <array>
        <string>/Users/gezhigang/weixin/run.sh</string>
    </array>

    <key>StartCalendarInterval</key>
    <dict>
        <key>Hour</key>
        <integer>6</integer>
        <key>Minute</key>
        <integer>0</integer>
    </dict>

    <key>StandardOutPath</key>
    <string>/Users/gezhigang/weixin/logs/daily.log</string>
    <key>StandardErrorPath</key>
    <string>/Users/gezhigang/weixin/logs/daily-error.log</string>

    <key>WorkingDirectory</key>
    <string>/Users/gezhigang/weixin</string>

    <key>EnvironmentVariables</key>
    <dict>
        <key>PATH</key>
        <string>/opt/homebrew/bin:/usr/local/bin:/usr/bin:/bin</string>
    </dict>
</dict>
</plist>
